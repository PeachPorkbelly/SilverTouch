<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mission.burger.M0_03">

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/pager"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="220dp"
        android:layout_marginStart="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/hint"
        android:layout_width="150dp"
        android:layout_height="54dp"
        android:layout_marginTop="80dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/misson_btn_hint"
        tools:layout_editor_absoluteY="171dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/line3" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="299dp"
        android:layout_height="51dp"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/kiosk_0021_kiosk_misson"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:id="@+id/disabled"
        android:layout_width="39dp"
        android:layout_height="48dp"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="81dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mission_bg_disabled" />

    <ImageButton
        android:id="@+id/lens"
        android:layout_width="39dp"
        android:layout_height="48dp"
        android:layout_marginLeft="7dp"
        android:layout_marginBottom="81dp"
        app:srcCompat="@drawable/lens"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@id/disabled"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="218dp"
        tools:layout_editor_absoluteY="611dp" />

    <ImageButton
        android:id="@+id/call"
        android:layout_width="39dp"
        android:layout_height="48dp"
        android:layout_marginLeft="7dp"
        android:layout_marginBottom="81dp"
        app:srcCompat="@drawable/mission_bg_call"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@id/lens"
        app:layout_constraintBottom_toBottomOf="parent" />

    <ImageButton
        android:id="@+id/cancel"
        android:layout_width="99dp"
        android:layout_height="54dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="81dp"
        app:srcCompat="@drawable/cancel"
        android:background="@android:color/transparent"
        app:layout_constraintLeft_toRightOf="@id/call"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent" />

    <ImageButton
        android:id="@+id/pay"
        android:layout_width="99dp"
        android:layout_height="54dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="81dp"
        app:srcCompat="@drawable/pay"
        app:layout_constraintLeft_toRightOf="@id/cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@android:color/transparent"
        android:scaleType="fitXY" />

    <ImageButton
        android:id="@+id/recomm"
        android:layout_width="60dp"
        android:layout_height="21dp"
        android:layout_marginTop="240dp"
        android:layout_marginStart="27dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintRight_toRightOf="@id/hamburgerw"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kiosk3_0019_recomm"
        tools:layout_editor_absoluteY="165dp" />

    <ImageButton
        android:id="@+id/hamburgerw"
        android:layout_width="90dp"
        android:layout_height="48dp"
        android:layout_marginLeft="81dp"
        android:layout_marginTop="224dp"
        android:layout_marginBottom="458dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="@id/recomm"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hamburgerw" />

    <ImageButton
        android:id="@+id/desert"
        android:layout_width="60dp"
        android:layout_height="21dp"
        android:layout_marginTop="240dp"
        android:layout_marginBottom="458dp"
        android:layout_marginLeft="20dp"
        app:srcCompat="@drawable/kiosk3_0017_kiosk_menu3"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@id/hamburgerw"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="189dp" />

    <ImageButton
        android:id="@+id/drink"
        android:layout_width="60dp"
        android:layout_height="21dp"
        app:srcCompat="@drawable/kiosk3_0016_kiosk_menu4"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toRightOf="@id/desert"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="240dp"
        tools:layout_editor_absoluteX="311dp"
        tools:layout_editor_absoluteY="166dp" />


</androidx.constraintlayout.widget.ConstraintLayout>