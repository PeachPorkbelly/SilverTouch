<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/inssatest_choice2_exit"
        android:layout_width="51dp"
        android:layout_height="41dp"
        android:layout_marginTop="4dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/misson_exit" />

    <ImageView
        android:id="@+id/inssatest_choice2_questionOrder"
        android:layout_width="84dp"
        android:layout_height="24dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inssatest_choice2_exit"
        app:srcCompat="@drawable/inssatest_t01" />

    <ImageButton
        android:id="@+id/inssatest_choice2_questionTitle"
        android:layout_width="wrap_content"
        android:layout_height="94dp"
        android:layout_marginTop="36dp"
        android:scaleType="fitCenter"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inssatest_choice2_questionOrder"
        app:srcCompat="@drawable/inssatest_q01" />



    <ImageButton
        android:id="@+id/inssatest_choice2_answer01"
        android:layout_width="330dp"
        android:layout_height="152dp"
        android:layout_marginTop="104dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inssatest_choice2_questionTitle"
        app:srcCompat="@drawable/inssatest_answer_icon_data" />

    <ImageButton
        android:id="@+id/inssatest_choice2_answer02"
        android:layout_width="330dp"
        android:layout_height="152dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitCenter"
        android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inssatest_choice2_answer01"
        app:srcCompat="@drawable/inssatest_answer_icon_wifi" />

</androidx.constraintlayout.widget.ConstraintLayout>