<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mission.burger.M0_02">
    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="297dp"
        android:layout_height="51dp"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/mission_bg_kiosk_0021_kiosk_misson"
        app:layout_constraintTop_toBottomOf="@id/frameLayout3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:id="@+id/hint"
        android:layout_width="136dp"
        android:layout_height="48dp"
        android:layout_marginTop="10dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView7"
        app:srcCompat="@drawable/misson_btn_hint"
        tools:layout_editor_absoluteY="171dp" />

    <ImageButton
        android:id="@+id/bg0_x2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/misson_exit" />

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bg0_x2">

        <ImageView
            android:id="@+id/bg_m0_02_mission_order_1"
            android:layout_width="84dp"
            android:layout_height="24dp"
            app:srcCompat="@drawable/mission_map_icon_misson_1" />

        <ImageView
            android:id="@+id/bg_m0_02_mission_order_2"
            android:layout_width="84dp"
            android:layout_height="24dp"
            app:srcCompat="@drawable/mission_map_icon_misson_2" />

        <ImageView
            android:id="@+id/bg_m0_02_mission_order_3"
            android:layout_width="84dp"
            android:layout_height="24dp"
            app:srcCompat="@drawable/mission_map_icon_misson_3"
            tools:layout_editor_absoluteX="155dp"
            tools:layout_editor_absoluteY="74dp" />

    </FrameLayout>

    <ImageView
        android:id="@+id/line"
        android:layout_width="350dp"
        android:layout_height="501dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/mission_bg_line4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hint" />


    <ImageButton
        android:id="@+id/disabled"
        android:layout_width="39dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="10dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:srcCompat="@drawable/mission_bg_disabled" />

    <ImageButton
        android:id="@+id/lens"
        android:layout_width="39dp"
        android:layout_height="48dp"
        android:layout_marginLeft="7dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/mission_bg_lens"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintLeft_toRightOf="@id/disabled" />

    <ImageButton
        android:id="@+id/call"
        android:layout_width="39dp"
        android:layout_height="48dp"
        android:layout_marginLeft="7dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/mission_bg_call"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@id/lens"
        app:layout_constraintBottom_toBottomOf="@id/line" />

    <ImageButton
        android:id="@+id/cancel"
        android:layout_width="80dp"
        android:layout_height="44dp"
        android:layout_marginLeft="30dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/mission_bg_cancel"
        android:background="@android:color/transparent"
        app:layout_constraintLeft_toRightOf="@id/call"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/line" />

    <ImageButton
        android:id="@+id/pay"
        android:layout_width="80dp"
        android:layout_height="44dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/mission_bg_pay"
        app:layout_constraintLeft_toRightOf="@id/cancel"
        app:layout_constraintBottom_toBottomOf="@id/line"
        android:background="@android:color/transparent"
        android:scaleType="fitXY" />

    <ImageButton
        android:id="@+id/recommw"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="27dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintRight_toRightOf="@id/hamburger"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/line"
        app:srcCompat="@drawable/mission_bg_recommw"
        tools:layout_editor_absoluteY="165dp" />

    <ImageButton
        android:id="@+id/hamburger"
        android:layout_width="38dp"
        android:layout_height="14dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="458dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="@id/recommw"
        app:layout_constraintTop_toTopOf="@id/line"
        app:srcCompat="@drawable/mission_bg_kiosk3_0018_kiosk_menu2" />

    <ImageButton
        android:id="@+id/desert"
        android:layout_width="70dp"
        android:layout_height="14dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="458dp"
        android:layout_marginLeft="30dp"
        app:srcCompat="@drawable/mission_bg_kiosk3_0017_kiosk_menu3"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@id/hamburger"
        app:layout_constraintTop_toTopOf="@id/line"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="189dp" />

    <ImageButton
        android:id="@+id/drink"
        android:layout_width="58dp"
        android:layout_height="14dp"
        app:srcCompat="@drawable/mission_bg_kiosk3_0016_kiosk_menu4"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toRightOf="@id/desert"
        app:layout_constraintTop_toTopOf="@id/line"
        android:layout_marginTop="25dp"
        tools:layout_editor_absoluteX="311dp"
        tools:layout_editor_absoluteY="166dp" />

    <ImageButton
        android:id="@+id/imageButton16"
        android:layout_width="145dp"
        android:layout_height="72dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:layout_marginTop="24dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toBottomOf="@id/recommw"
        app:layout_constraintStart_toStartOf="@id/line"
        app:srcCompat="@drawable/mission_bg_doubleburger"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="276dp" />



</androidx.constraintlayout.widget.ConstraintLayout>