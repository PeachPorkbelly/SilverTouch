<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/m0_03_background">


    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="284px"
        android:layout_marginTop="268px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/m0_03_mission_order_1"
            android:layout_width="90.2dp"
            android:layout_height="25.5dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/misson_contents_1" />

        <ImageView
            android:id="@+id/m0_03_mission_order_2"
            android:layout_width="90.2dp"
            android:layout_height="25.5dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/misson_contents_2" />

        <ImageView
            android:id="@+id/m0_03_mission_order_3"
            android:layout_width="90.2dp"
            android:layout_height="25.5dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/misson_contents_3" />

    </FrameLayout>

    <ImageButton
