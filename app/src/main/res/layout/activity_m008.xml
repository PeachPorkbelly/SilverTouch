<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mission.burger.M0_03">

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/pager"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="220dp"
        android:layout_marginStart="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

    </androidx.viewpager.widget.ViewPager>

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="299dp"
        android:layout_height="51dp"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/kiosk_0021_kiosk_misson"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:id="@+id/hint"
        android:layout_width="150dp"
        android:layout_height="54dp"
        android:layout_marginTop="80dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/misson_btn_hint"
        tools:layout_editor_absoluteY="171dp" />

    <ImageView
        android:id="@+id/line"
        android:layout_width="350dp"
        android:layout_height="501dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/mission_bg_line5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hint" />


    <ImageButton
        android:id="@+id/disabled"
        android:layout_width="39dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="10dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintStart_toStartOf="@id/line"
        app:srcCompat="@drawable/mission_bg_disabled" />

    <ImageButton
        android:id="@+id/lens"
        android:layout_width="39dp"
        android:layout_height="48dp"
        android:layout_marginLeft="7dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/lens"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/line"
        app:layout_constraintLeft_toRightOf="@id/disabled" />

    <ImageButton
        android:id="@+id/call"
        android:layout_width="39dp"
        android:layout_height="48dp"
        android:layout_marginLeft="7dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/mission_bg_call"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@id/lens"
        app:layout_constraintBottom_toBottomOf="@id/line" />

    <ImageButton
        android:id="@+id/cancel"
        android:layout_width="80dp"
        android:layout_height="44dp"
        android:layout_marginLeft="30dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/cancel"
        android:background="@android:color/transparent"
        app:layout_constraintLeft_toRightOf="@id/call"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@id/line" />

    <ImageButton
        android:id="@+id/pay"
        android:layout_width="80dp"
        android:layout_height="44dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/pay"
        app:layout_constraintLeft_toRightOf="@id/cancel"
        app:layout_constraintBottom_toBottomOf="@id/line"
        android:background="@android:color/transparent"
        android:scaleType="fitXY" />

    <ImageButton
        android:id="@+id/recomm"
        android:layout_width="51dp"
        android:layout_height="14dp"
        android:layout_marginTop="25dp"
        android:layout_marginStart="20dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintRight_toRightOf="@id/hamburger"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        app:srcCompat="@drawable/kiosk3_0019_recomm"
        tools:layout_editor_absoluteY="165dp" />

    <ImageButton
        android:id="@+id/hamburger"
        android:layout_width="81dp"
        android:layout_height="40dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="458dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="@id/recomm"
        app:layout_constraintTop_toTopOf="@id/line"
        app:srcCompat="@drawable/hamburgerw" />

    <ImageButton
        android:id="@+id/desert"
        android:layout_width="70dp"
        android:layout_height="14dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="458dp"
        android:layout_marginLeft="20dp"
        app:srcCompat="@drawable/kiosk3_0017_kiosk_menu3"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@id/hamburger"
        app:layout_constraintTop_toTopOf="@id/line"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="189dp" />

    <ImageButton
        android:id="@+id/drink"
        android:layout_width="58dp"
        android:layout_height="14dp"
        app:srcCompat="@drawable/kiosk3_0016_kiosk_menu4"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toRightOf="@id/desert"
        app:layout_constraintTop_toTopOf="@id/line"
        android:layout_marginTop="25dp"
        tools:layout_editor_absoluteX="311dp"
        tools:layout_editor_absoluteY="166dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="346dp"
        android:layout_height="30dp"
        android:layout_marginTop="303dp"
        app:srcCompat="@drawable/count"
        app:layout_constraintStart_toStartOf="@id/line"
        app:layout_constraintTop_toTopOf="@id/line"
        />


</androidx.constraintlayout.widget.ConstraintLayout>